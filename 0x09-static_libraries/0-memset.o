#include "main.h"
/**
 * _memset - fill the memory with constant byte
 * @s: the first memory adress filled
 * @b: the desired value
 * @n: number of bytes to be changed
 *
 * Return: pointer to a memory area.
 */
char *_memset(char *s, char b, unsigned int n)
{
	int i = 0;

	for (; n > 0; i++)
	{
		s[i] = b;
		n--;
	}
	return (s);
}

